{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main-api/src",
  "monorepo": true,
  "root": "apps/main-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main-api/tsconfig.json"
  },
  "projects": {
    "main-api": {
      "type": "application",
      "root": "apps/main-api",
      "entryFile": "main",
      "sourceRoot": "apps/main-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main-api/tsconfig.json"
      }
    },
    "modules": {
      "type": "library",
      "root": "apps/libs/modules",
      "entryFile": "index",
      "sourceRoot": "apps/libs/modules",
      "compilerOptions": {
        "tsConfigPath": "apps/libs/tsconfig.json"
      }
    },
    "core": {
      "type": "library",
      "root": "apps/libs/core",
      "entryFile": "index",
      "sourceRoot": "apps/libs/core",
      "compilerOptions": {
        "tsConfigPath": "apps/libs/tsconfig.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "apps/libs/utils",
      "entryFile": "index",
      "sourceRoot": "apps/libs/utils",
      "compilerOptions": {
        "tsConfigPath": "apps/libs/tsconfig.json"
      }
    },
    "eslint": {
      "type": "library",
      "root": "apps/eslint",
      "entryFile": ".eslintrc.json",
      "sourceRoot": "apps/eslint"
    }
  }
}