{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main-api/src",
  "monorepo": true,
  "root": "apps/main-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main-api/tsconfig.json"
  },
  "projects": {
    "@app/main-api": {
      "type": "application",
      "root": "apps/main-api",
      "entryFile": "main",
      "sourceRoot": "apps/main-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main-api/tsconfig.json"
      }
    },
    "@app/other-api": {
      "type": "application",
      "root": "apps/other-api",
      "entryFile": "main",
      "sourceRoot": "apps/other-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/other-api/tsconfig.json"
      }
    },
    "@app/modules": {
      "type": "library",
      "root": "apps/libs/modules",
      "entryFile": "index",
      "sourceRoot": "apps/libs/modules",
      "compilerOptions": {
        "tsConfigPath": "apps/libs/tsconfig.json"
      }
    },
    "@app/core": {
      "type": "library",
      "root": "apps/libs/core",
      "entryFile": "index",
      "sourceRoot": "apps/libs/core",
      "compilerOptions": {
        "tsConfigPath": "apps/libs/tsconfig.json"
      }
    },
    "@app/utils": {
      "type": "library",
      "root": "apps/libs/utils",
      "entryFile": "index",
      "sourceRoot": "apps/libs/utils",
      "compilerOptions": {
        "tsConfigPath": "apps/libs/tsconfig.json"
      }
    },
    "@app/eslint.config": {
      "type": "library",
      "root": "apps/eslint",
      "entryFile": ".eslintrc.json",
      "sourceRoot": "apps/eslint"
    }
  }
}